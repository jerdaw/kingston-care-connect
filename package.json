    "postcss-import": "^16.1.1",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix",
      "vitest related --run"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
